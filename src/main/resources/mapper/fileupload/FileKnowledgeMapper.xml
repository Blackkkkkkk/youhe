<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youhe.mapperfile..FileKnowledgeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.youhe.entity.fileupload.FileKnowledge">
        <id column="ID" property="id" />
        <result column="REGISTER_DATE" property="registerDate" />
        <result column="FILE_NAME" property="fileName" />
        <result column="FILE_CATEGORY" property="fileCategory" />
        <result column="FILE_TYPE" property="fileType" />
        <result column="SAVE_FILE_NAME" property="saveFileName" />
        <result column="SERVER_ADDR" property="serverAddr" />
        <result column="FILE_SIZE" property="fileSize" />
        <result column="VERSION_NUMBER" property="versionNumber" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, REGISTER_DATE, FILE_NAME, FILE_CATEGORY, FILE_TYPE, SAVE_FILE_NAME, SERVER_ADDR, FILE_SIZE, VERSION_NUMBER
    </sql>

</mapper>
